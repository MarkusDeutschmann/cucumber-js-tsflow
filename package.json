{
  "name": "cucumber-tsflow-workspace",
  "version": "2.0.0",
  "private": true,
  "description": "Workspace for cucumber-tsflow",
  "repository": {
    "type": "git",
    "url": "https://github.com/timjroberts/cucumber-js-tsflow.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "postinstall": "lerna bootstrap",
    "precommit": "lint-staged && npm test",
    "set-packageversion": "node .build/setPackageVersion.js",
    "pretest": "npm run build",
    "test": "cucumber-js --require cucumber-tsflow-specs/dist cucumber-tsflow-specs "
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0 || ^8.0.0",
    "@types/node": "^10",
    "@types/underscore": "1.11.4",
    "jest": "^29.2.2",
    "lerna": "^6.0.1",
    "lint-staged": "^13.0.3",
    "nerdbank-gitversioning": "^3.5.119",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  }
}
